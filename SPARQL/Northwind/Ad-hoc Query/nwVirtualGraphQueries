SELECT (COUNT(?s) as ?numTriples)
{
  GRAPH <virtual://nwvg> 
{
    ?s ?p ?o .
}
}


SELECT *
{
  GRAPH <virtual://nwvg> 
{
    ?s a ?o .
}
}

SELECT *
{
    ?s a :product .
}

